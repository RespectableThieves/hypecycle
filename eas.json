{
  "cli": {
    "version": "3.9.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development",
        "ICON_COLOR": "#00D084"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview",
        "ICON_COLOR": "#FF5722"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_VARIANT": "production",
        "ICON_COLOR": "#000000"
      }
    }
  }
}
